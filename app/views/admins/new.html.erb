<h1>sign up admin</h1>





    <%= form_for @admin do |f| %>
      <p>
        <%= f.text_field :name %>
      </p>
      <p>
        <%= f.text_field :username %>
      </p>
      <p>
        <%= f.password_field :password %>
      </p>
      <p>
        <%= f.password_field :password_confirmation %>
      </p>
      <p class="button"><%= f.submit %></p>
<% end %>
